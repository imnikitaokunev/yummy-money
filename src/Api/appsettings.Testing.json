{
  "KeyVault": {
    "Vault": "yummymoney-kvtesting"
  },
  "ApplicationName": "yummymoney-api",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AzureBlobStorage": {
    "ConnectionString": "<set by keyvault>"
  },
  "JwtSettings": {
    "Secret": "string64baseMustBeHere",
    "TokenLifetimeInSeconds": 7200,
    "RefreshTokenLifetimeInSeconds": 8120
  },
  "SecuritySettings": {
    "SaltLength": 16
  },
  "ConnectionStrings": {
    "DefaultConnection": "<set by keyvault>"
  }
}