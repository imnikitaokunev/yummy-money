{
  "ApplicationName": "yummymoney-api",
  "KeyVault": {
    "Vault": ""
  },
  "Serilog": {
    "IncludeScopes": true,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AzureBlobStorage": {
    "ConnectionString": ""
  },
  "JwtSettings": {
    "Secret": "string64baseMustBeHere",
    "TokenLifetimeInSeconds": 7200,
    "RefreshTokenLifetimeInSeconds": 8120
  },
  "RedisCacheSettings": {
    "IsEnabled": true,
    "ConnectionString": "localhost:6379"
  },
  "ConnectionStrings": {
    "DefaultConnection": "data source=(local);Initial Catalog=YummyMoney;Integrated Security=True;"
  }
}